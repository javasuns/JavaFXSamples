<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<HBox fx:id="headerPane" prefWidth="500.0" stylesheets="@Header.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javasuns.samples.controller.header.HeaderController">
   <styleClass>
      <String fx:value="header" />
   </styleClass>
   <children>
      <Button fx:id="btnBack" cache="true" cacheHint="SPEED" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="0.0" mnemonicParsing="false" onAction="#handleButtonAction" prefWidth="0.0" style="-fx-alignment: CENTER_LEFT;" HBox.hgrow="ALWAYS">
         <graphic>
            <Region prefHeight="200.0" prefWidth="200.0">
               <styleClass>
                  <String fx:value="icon_back_arrow" />
                  <String fx:value="header_buttons" />
               </styleClass></Region>
         </graphic>
      </Button>
      <Label fx:id="lblTitle" alignment="CENTER" cache="true" cacheHint="SPEED" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="&lt;Title&gt;" />
      <Region maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
   </children>
</HBox>
