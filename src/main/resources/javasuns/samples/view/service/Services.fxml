<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../../css/Services.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javasuns.samples.controller.service.ServicesController">
   <children>
      <Button fx:id="btnDeviceInfo" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#buttonPressed">
         <graphic>
            <HBox alignment="CENTER" styleClass="row">
               <children>
                  <Region maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT">
                     <styleClass>
                        <String fx:value="svg_device" />
                        <String fx:value="row_icon" />
                     </styleClass>
                  </Region>
                  <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="increased_font" text="Device Info" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets bottom="5.0" right="10.0" top="5.0" />
                     </HBox.margin>
                  </Label>
                  <Region maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT" styleClass="icon_left_arrow" />
               </children>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </HBox>
         </graphic>
      </Button>
      <Button fx:id="btnShareContent" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#buttonPressed">
         <graphic>
            <HBox alignment="CENTER" styleClass="row">
               <children>
                  <Region maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT">
                     <styleClass>
                        <String fx:value="svg_share" />
                        <String fx:value="row_icon" />
                     </styleClass>
                  </Region>
                  <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="increased_font" text="Share Content" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets bottom="5.0" right="10.0" top="5.0" />
                     </HBox.margin>
                  </Label>
                  <Region maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT" styleClass="icon_left_arrow" />
               </children>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </HBox>
         </graphic>
      </Button>
      <Button fx:id="btnBrowse" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#buttonPressed">
         <graphic>
            <HBox alignment="CENTER" styleClass="row">
               <children>
                  <Region maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT">
                     <styleClass>
                        <String fx:value="svg_browser" />
                        <String fx:value="row_icon" />
                     </styleClass>
                  </Region>
                  <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="increased_font" text="Browser" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets bottom="5.0" right="10.0" top="5.0" />
                     </HBox.margin>
                  </Label>
                  <Region maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT" styleClass="icon_left_arrow" />
               </children>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </HBox>
         </graphic>
      </Button>
      <Button fx:id="btnPushNotifications" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#buttonPressed">
         <graphic>
            <HBox alignment="CENTER" styleClass="row">
               <children>
                  <Region maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT" rotate="180.0">
                     <styleClass>
                        <String fx:value="svg_notifications" />
                        <String fx:value="row_icon" />
                     </styleClass>
                  </Region>
                  <Label maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" styleClass="increased_font" text="Push Notifications" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets bottom="5.0" right="10.0" top="5.0" />
                     </HBox.margin>
                  </Label>
                  <Region maxWidth="1.7976931348623157E308" nodeOrientation="RIGHT_TO_LEFT" styleClass="icon_left_arrow" />
               </children>
               <padding>
                  <Insets left="10.0" right="10.0" />
               </padding>
            </HBox>
         </graphic>
      </Button>
      <StackPane maxHeight="1.7976931348623157E308" style="-fx-background-color: white;" VBox.vgrow="ALWAYS">
         <children>
            <Region maxWidth="1.7976931348623157E308">
               <styleClass>
                  <String fx:value="img_javasuns_logo" />
                  <String fx:value="size_image_large" />
               </styleClass>
            </Region>
         </children>
      </StackPane>
   </children>
</VBox>
